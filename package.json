{
	"name": "ott",
	"version": "1.0.0",
	"description": "Private video streaming",
	"main": "./dist/index.js",
	"type": "module",
	"scripts": {
		"build": "npx tsc",
		"lint": "eslint --ignore-path .eslintignore --ext .js,.ts .",
		"format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
		"start": "node dist/index.js",
		"dev": "concurrently \"npx tsc --watch\" \"nodemon -q dist/index.js\""
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/saisaran96/private-ott.git"
	},
	"author": "",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/saisaran96/private-ott/issues"
	},
	"homepage": "https://github.com/saisaran96/private-ott#readme",
	"dependencies": {
		"dotenv": "^16.3.1",
		"express": "^4.18.2"
	},
	"devDependencies": {
		"@types/express": "^4.17.17",
		"@types/node": "^20.5.3",
		"@typescript-eslint/eslint-plugin": "^6.4.1",
		"@typescript-eslint/parser": "^6.4.1",
		"concurrently": "^8.2.1",
		"eslint": "^8.47.0",
		"nodemon": "^3.0.1",
		"prettier": "^3.0.2",
		"typescript": "^5.1.6"
	}
}
